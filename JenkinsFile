pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        git branch: 'CICD-using-Git,-Jenkins-&-Maven', url: 'https://github.com/tspoorthyreddy/CICD-with-Git-Jenkins-Ansible-K8s.git'
      }
    }
    stage('Build and Test') {
      steps {
        sh 'mvn install'
      }
    }
  }
}
